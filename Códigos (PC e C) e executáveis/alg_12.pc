inteiro principal () {
    inteiro nota;
    escreva ("Entre com a nota do aluno:");
    leia ("%inteiro", nota);
    escolha (nota){
        caso 0, 1, 2 :
            escreva ("Reprovado.\n");
        caso 4, 5, 6, 7:
            escreva ("Em final.\n");
        caso 8, 9, 10:
            escreva ("Aprovado.\n");
        padrao:
            escreva ("Nota invalida.\n");
    }
}
