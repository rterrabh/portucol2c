inteiro fibIterativo (inteiro n) {

    inteiro i, j, k, m;
    i = 1;
    j = 0;

    para (k=1 ; n ; 1) {
        m = i + j;
        i = j;
        j = m;
    }
    retorne j;
}

inteiro principal () {
    inteiro n;
    escreva ("Digite um numero ");
    leia ("%inteiro", n);
    escreva ("%inteiro", fibIterativo(n));
}